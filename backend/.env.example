# Database
POSTGRES_USER=smart_mirror
POSTGRES_PASSWORD=your_password_here
POSTGRES_DB=smart_mirror_db
DATABASE_URL=postgresql://smart_mirror:your_password_here@postgres:5432/smart_mirror_db

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=your_redis_password_here

# Security
SECRET_KEY=your_secret_key_here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Kling AI API
KLING_API_KEY=your_kling_api_key_here
KLING_API_URL=https://api.kling.ai/v1.5

# Model Settings
MODEL_PATH=/app/ai-model/model.h5
MODEL_VERSION=1.0

# Logging
LOG_LEVEL=INFO
LOG_FILE=/app/logs/app.log

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000
